<link rel="stylesheet" href="<%- theme.jsDelivr.url %><%- theme.libs.css.dplayer %>">
<div class="video-player">
    <% if (theme.video.showTitle) { %>
    <div class="title center-align">
        <i class="fas fa-video-camera"></i>&nbsp;&nbsp;<%- theme.video.title %>
    </div>
    <% } %>
    <div class="row">
        <div class="col l8 offset-l2 m10 offset-m1 s12">
            
            
                    <iframe src="//player.bilibili.com/player.html?aid=583518428&bvid=BV1wz4y1R7Xk&cid=203507198&page=1" style="width:674px;height: 405px;"  scrolling="no" border="0" frameborder="no" framespacing="0" allowfullscreen="true"> </iframe>    
            
        </div>
    </div>
</div>
